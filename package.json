  "main": "server.js",
  "scripts": {
    "start": "npm run build && cd server && node server.js",
    "start:prod": "npm run build && node server.js",
    "start:dev": "concurrently \"npm run server\" \"cd client && npm run start\"",
    "server": "nodemon server.js --ignore client",
